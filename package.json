{
  "name": "angular.dev",
  "version": "0.0.0",
  "scripts": {
    "npm:install:global:ci": "npm install --location=global @angular/cli http-server json-server",
    "npm:install:global:all": "npm install --location=global @angular/cli http-server json-server open-cli",
    "setup": "npm run npm:install:global:all",
    "setup:ci": "npm run npm:install:global:ci",
    "ng": "ng",
    "ng:serve": "npm run ng -- serve",
    "ng:build": "npm run ng -- build",
    "ng:build:watch": "npm run ng:build -- --watch --configuration development",
    "dist": "ng build",
    "dist:development": "npm run dist -- --configuration development",
    "dist:production": "npm run dist -- --configuration production",
    "browser:80": "open-cli http://localhost:80",
    "browser:4200": "open-cli http://localhost:4200",
    "browser:3000": "open-cli http://localhost:3000/locations",
    "http-server": "http-server ./dist",
    "json-server": "json-server --watch assets/json-server/first-app.json --port 3000",
    "prejson-server:local": "npm run browser:3000",
    "json-server:local": "npm run json-server",
    "json-server:docker": "npm run json-server -- --host 0.0.0.0",
    "docker:compose:up": "docker compose up --build --remove-orphans",
    "start:dev": "npm run ng:serve",
    "prestart:dist": "npm run dist",
    "start:dist": "npm run http-server",
    "prestart:docker": "npm run browser:80",
    "start:docker": "npm run docker:compose:up",
    "start": "npm run start:docker"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/router": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^16.11.35",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~5.6.0",
    "jasmine-marbles": "~0.9.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.0",
    "typescript": "~5.8.0"
  }
}
